{
    "name": "gg",
    "version": "0.0.1",
    "description": "REST API documentation for the Zoykee Scrum Management",
    "main": "index.js",
    "repository": "https://github.com/ejoo/express-mysql.git",
    "author": "",
    "license": "MIT",
    "scripts": {
      "prestart": "yarn run build",
      "start": "cross-env NODE_ENV=production && yarn run serve ",
      "test-server": "cross-env ENV=test && nodemon app --exec \"node -r dotenv/config -r babel-register\"",
      "clean": "rimraf build",
      "build": "npm run clean && npm run build-ts",
      "db": "./node_modules/.bin/knex",
      "generate": "plop --plopfile internals/generators/index.js --require babel-core/register",
      "docs": "./node_modules/.bin/apidoc -i app/ -o docs/",
      "pretest": "cross-env NODE_ENV=test && cross-env NODE_ENV=test",
      "test": "cross-env NODE_ENV=test  jest",
      "test:watch": "cross-env NODE_ENV=test  jest \"app/**/*-test.ts\" --require babel-core/register --recursive --timeout 10000 --exit || true",
      "env-test": "cross-env NODE_ENV=test node app/test-env.js",
      "serve": "node build/index.js",
      "jest": "jest",
      "watch-node": "nodemon build/index.js",
      "watch-ts": "tsc -w",
      "tslint": "tslint -c tslint.json -p tsconfig.json",
      "debug": "npm run build && npm run watch-debug",
      "serve-debug": "nodemon --inspect build/server.js",
      "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\"  \"npm run watch-ts\" \"npm run serve-debug\"",
      "build-ts": "node_modules/.bin/tsc",
      "dev": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\"  \"npm run watch-ts\" \"npm run watch-node\"",
      "prettier:base": "prettier --parser typescript --single-quote",
      "precommit": "pretty-quick --staged"
    },
    "dependencies": {
      "@sendgrid/mail": "^6.2.1",
      "@sentry/node": "^4.6.4",
      "@types/jest": "^24.0.3",
      "@types/multer-s3": "^2.7.6",
      "apidoc": "^0.17.6",
      "app-root-path": "^2.1.0",
      "aws-sdk": "^2.392.0",
      "bcrypt": "^1.0.3",
      "body-parser": "^1.18.2",
      "celebrate": "^7.0.4",
      "chai": "^4.1.2",
      "chai-http": "^4.0.0",
      "chalk-logger": "^0.0.1",
      "cors": "^2.8.4",
      "crypto": "^1.0.1",
      "crypto-js": "^3.1.9-1",
      "dotenv": "^4.0.0",
      "express": "^4.16.2",
      "express-validation": "^1.0.2",
      "faker": "^4.1.0",
      "fs": "^0.0.1-security",
      "global": "^4.3.2",
      "helmet": "^3.10.0",
      "http-codes": "^1.0.0",
      "ip": "^1.1.5",
      "jest": "^23.0.0",
      "joi": "^13.1.2",
      "jsonwebtoken": "^8.2.0",
      "knex": "^0.16.3",
      "minimist": "^1.2.0",
      "module-alias": "^2.1.0",
      "moment": "^2.20.1",
      "morgan": "^1.9.0",
      "multer": "^1.3.0",
      "multer-s3": "^2.9.0",
      "mysql2": "^1.5.1",
      "nodemailer": "^4.6.7",
      "nodemon": "^1.17.2",
      "objection": "^1.2.2",
      "path": "^0.12.7",
      "pg": "^7.8.0",
      "pretty-quick": "^1.7.0",
      "query-string": "^6.1.0",
      "redis": "^2.8.0",
      "s3fs": "^2.5.0",
      "socket.io": "^2.2.0",
      "sparkpost": "^2.1.2",
      "supertest": "^3.0.0",
      "ts-jest": "^23.10.5",
      "winston": "^2.4.4"
    },
    "devDependencies": {
      "@types/app-root-path": "^1.2.4",
      "@types/bcrypt": "^2.0.0",
      "@types/body-parser": "^1.16.8",
      "@types/cors": "^2.8.4",
      "@types/dotenv": "^4.0.3",
      "@types/express": "^4.16.0",
      "@types/helmet": "^0.0.38",
      "@types/http-codes": "^1.0.1",
      "@types/ip": "^0.0.31",
      "@types/joi": "^13.4.0",
      "@types/jsonwebtoken": "^7.2.8",
      "@types/knex": "^0.14.19",
      "@types/morgan": "^1.7.35",
      "@types/multer": "^1.3.7",
      "@types/node": "^9.4.6",
      "@types/nodemailer": "^4.3.4",
      "@types/query-string": "^6.1.0",
      "@types/redis": "^2.8.6",
      "@types/supertest": "^2.0.5",
      "@types/winston": "^2.4.4",
      "babel-cli": "^6.26.0",
      "babel-preset-env": "^1.6.1",
      "babel-preset-flow": "^6.23.0",
      "babel-preset-stage-0": "^6.24.1",
      "babel-register": "^6.26.0",
      "concurrently": "^3.5.1",
      "cross-env": "^5.1.3",
      "mocha": "^5.0.5",
      "plop": "^2.0.0",
      "prettier": "^1.14.3",
      "rimraf": "^2.6.2",
      "supertest": "^3.0.0",
      "ts-lint": "^4.5.1",
      "typescript": "^3.0.1"
    },
    "_moduleAliases": {
      "@root": ".",
      "app": "build/app",
      "models": "build/models",
      "config": "build/config",
      "modules": "build/modules",
      "utils": "build/utils",
      "common": "build/common"
    }
  }