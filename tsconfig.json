{
	"compilerOptions": {
		"module": "commonjs",
		"esModuleInterop": true,
		"target": "es2015",
		"noImplicitAny": false,
		"moduleResolution": "node",
		"sourceMap": true,
		"outDir": "lib",
		"baseUrl": "./",
		"lib": ["es6", "dom", "esnext.asynciterable"],
		"types": [
			"node",
			"es2017",
			"dom",
			"esnext"
		]
	},
	"include": [
		"src/**/*"
	]
}